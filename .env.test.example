# SD-Ghost Protocol Test Environment
# Copy to .env.test and fill with actual values

# Application
NODE_ENV=test
PORT=3000
LOG_LEVEL=debug

# Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Hostinger API (optional for testing)
HOSTINGER_API_KEY=your_hostinger_api_key_here
HOSTINGER_EMAIL=your-email@domain.com

# AI API Keys
OPENAI_API_KEY=sk-proj-REPLACE_WITH_YOUR_OPENAI_KEY
ANTHROPIC_API_KEY=sk-ant-api03-REPLACE_WITH_YOUR_ANTHROPIC_KEY
OPENROUTER_API_KEY=sk-or-v1-REPLACE_WITH_YOUR_OPENROUTER_KEY

# Security
CORS_ORIGIN=http://localhost:8080
JWT_SECRET=your_jwt_secret_key_minimum_32_characters_long

# Features (Test Environment)
ENABLE_TOOL_ORCHESTRATION=true
MAX_TOOL_ITERATIONS=3
MEMORY_SEARCH_THRESHOLD=0.7
ENABLE_WEBSOCKETS=true
ENABLE_HEALTH_CHECKS=true

# Performance (Test Settings)
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=15000
MEMORY_CACHE_TTL=60

# Monitoring
ENABLE_METRICS=false
METRICS_PORT=9091
